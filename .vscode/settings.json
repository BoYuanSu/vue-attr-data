{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "eslint.options": {
        "extensions": [
            ".js",
            ".ts"
        ]
    },
    "eslint.validate": [
        "javascript",
        "typescript",
        "typescriptreact"
    ],
    "files.exclude": {
        "**/dist/**": false,
    }
}
